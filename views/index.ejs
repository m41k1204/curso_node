<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <p>Messages: </p>
    <ul>
      <% messages.forEach(result => { %>
      <% for (let word in result) { %>
          <li> <%= result[word] %> </li>  
      <% } %> 
      <% }) %>
    </ul>
  </body>
</html>
